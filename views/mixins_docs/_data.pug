mixin data()
  
  p.card-message
    | !{l.data_1} 
  
  h5 
    .headerLink !{l.data_participants_title}
  p.card-message 
    | #{l.data_participants_1}

  p.card-message 
    | #{l.data_participants_2}

  p.card-message 
    | #{l.data_participants_3}
    
  h5 
    .headerLink #{l.data_history_title}
    
  p.card-message 
    | #{l.data_history_1}
    
  ol
    li 
      p.card-message #{l.data_history_2_1}
    li 
      p.card-message #{l.data_history_2_2}
    li 
      p.card-message #{l.data_history_2_3}
    li 
      p.card-message #{l.data_history_2_4}
  
  p.card-message 
    | #{l.data_history_3}
    
